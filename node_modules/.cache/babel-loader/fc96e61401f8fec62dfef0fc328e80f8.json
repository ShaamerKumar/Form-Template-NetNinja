sError:'',\n            \n            \n\n        }\n    },\n    methods: {\n        addSkill(e) {\n            if((e.key===',' || e.key==='Enter') && this.tempSkill ){\n                if(!this.skills.includes(this.tempSkill)){\n                    this.skills.push(this.tempSkill)\n\n                }\n                \n                this.tempSkill = ''\n            }\n\n\n        },\n        delSkill(skill){\n\n            this.skills = this.skills.filter((item) => {\n                return skill !== item\n            })\n            \n            \n            \n            \n        },\n        handleSubmit() {\n            this.passError = this.password.length > 5 ? \n            '' : 'Password Must be 6 chars long'\n\n            if(!this.passError){\n                console.log(this.email)\n                console.log(this.password)\n                \n            }\n        \n\n\n        }\n    }\n\n}\n</script>\n\n<style>\n    form{\n        max-width: 420px;\n        margin: 30px auto;\n        background: white;\n        text-align: left;\n        padding: 40px;\n        border-radius: 10px;\n    }\n    label{\n        color:#aaa;\n        display:inline-block;\n        margin: 25px 0 15px;\n        font-size: 0.6em;\n        text-transform: uppercase;\n        letter-spacing: 1px;\n        font-weight: bold;\n\n    }\n    input, select{\n        display:block;\n        padding: 10px 6px;\n        width: 100%;\n        box-sizing: border-box;\n        border: none;\n        border-bottom: 1px solid #ddd;\n        color: #555;\n\n    }\n    input[type=\"checkbox\"]{\n        display: inline-block;\n        width: 16px;\n        margin: 0 10px 0 0;\n        position: relative;\n        top: 2px;\n\n    }\n    .pill{\n        display: inline-block;\n        margin: 20px 10px 0 0 ;\n        padding: 6px 12px;\n        background: #eee;\n        border-radius: 20px;\n        font-size: 12px;\n        letter-spacing: 1px;\n        font-weight: bold;\n        color: #777;\n        cursor: pointer;\n    }\n    button{\n        background: maroon;\n        border: 0;\n        padding:10px 20px;\n        margin-top: 20px;\n        color: white;\n        border-radius: 20px;\n    }\n    .submit{\n        text-align: center;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}